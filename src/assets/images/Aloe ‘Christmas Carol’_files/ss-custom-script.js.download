function repositionErrorMessage(){jQuery(function($){if(typeof($('ul.error-messages').offset())!='undefined'){$('ul.error-messages li').each(function(event){if($(this).text().indexOf('Billing First name')>=0){$(this).appendTo($('input.billing_first_name').closest('.form-row'));console.log($(this).text());}});}});}
jQuery(document).ready(function($){var cur_date=new Date();$('#footer .current-year').text(cur_date.getFullYear());if(window.location.pathname.indexOf('/checkout')>=0){if($.cookie('checkout-action')!==undefined&&$.cookie('checkout-action')=='login'&&typeof($('.woocommerce-error').offset())!='undefined'){$('form.woocommerce-form.woocommerce-form-login.login').show();$.removeCookie('checkout-action');}else if($.cookie('checkout-action')!==undefined&&$.cookie('checkout-action')=='place-order'&&typeof($('.woocommerce-error').offset())!='undefined'){$.removeCookie('checkout-action');}}});if(window.location.pathname.indexOf('/checkout')>=0){jQuery(document).on('click','a.woocommerce-form.login',function(event){jQuery.cookie('checkout-action','login',{expires:1,path:'/checkout'});});jQuery(document).on('click','form.checkout.woocommerce-checkout *[type="submit"]',function(event){jQuery.cookie('checkout-action','place-order',{expires:1,path:'/checkout'})});}